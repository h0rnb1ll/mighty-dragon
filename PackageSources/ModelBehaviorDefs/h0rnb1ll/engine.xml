                <Code>#ANIM_CODE#</Code>
                <Lag>#ANIM_LAG#</Lag>
                <Wrap>#ANIM_WRAP#</Wrap>
                <Delta>#ANIM_DELTA#</Delta>
                <BlendWeight>#ANIM_BLENDWEIGHT#</BlendWeight>
            </Parameter>
        </Animation>
    </Template>

