	<Template Name="HBL_Softkey_Template">
		<DefaultTemplateParameters>
			<PREFIX>#DISPLAY#</PREFIX>
			<DISPLAY_POS>L</DISPLAY_POS>
			<KEY_PREFIX>#DISPLAY#_#ID#</KEY_PREFIX>
			<Condition>
				<Test>
					<Lower>
						<Value>KEY_ID</Value>
						<Number>10</Number>
					</Lower>
				</Test>
				<True>
					<UID>00#KEY_ID#</UID>
				</True>
				<False>
					<UID>0#KEY_ID#</UID>
				</False>
			</Condition>
		</DefaultTemplateParameters>
