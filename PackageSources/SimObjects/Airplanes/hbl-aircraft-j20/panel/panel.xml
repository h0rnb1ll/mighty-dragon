	<FuelDisplay>
		<ColumnsGroup id="fuel">
			<Column id="CAS" width="33.3">
				<CAS nbMessages="10"/>
			</Column>
			<Column width="66.6">
				<Gauge>
					<Type>LongitudeFuel</Type>
					<Title>Test</Title>
					<Unit>USG</Unit>
					<Minimum>0</Minimum>
					<Maximum>150</Maximum>
					<Value>
						<Simvar name="FUEL LEFT QUANTITY" unit="gallons"/>
					</Value>
					<Value2>
						<Simvar name="FUEL RIGHT QUANTITY" unit="gallons"/>
					</Value2>
					<ColorLine>
						<Color>red</Color>
						<Position>0</Position>
					</ColorLine>
					<ColorZone>
						<Color>yellow</Color>
						<Begin>0</Begin>
						<End>9</End>
					</ColorZone>
					<ColorZone>
						<Color>green</Color>
						<Begin>9</Begin>
						<End>150</End>
					</ColorZone>
					<GraduationLength text="True">25</GraduationLength>
					<EndText></EndText>
				</Gauge>
				
				<Gauge>
					<Type>FlapsSpeedbrakes</Type>
					<Title>FLAPS</Title>
					<ID>Turbo_Flaps</ID>
					<Minimum>0</Minimum>
					<Maximum>35</Maximum>
					<TakeOff>10</TakeOff>
					<Value>
						<Simvar name="TRAILING EDGE FLAPS LEFT ANGLE" unit="degree"/>
					</Value>
					<Value2>
						<Multiply>
							<Simvar name="SPOILERS LEFT POSITION" unit="number"/>
							<Constant>-100.0</Constant>
						</Multiply>
					</Value2>
				</Gauge>
				
				<ColumnsGroup id="Trim and Gear">
					<Column id="Trim" width="66.6">
						<ColumnsGroup>
							<Column id="Elev Trim">
								<Gauge>
									<Type>Vertical</Type>
									<Style>
										<CursorColor>aqua</CursorColor>
										<ValuePos>None</ValuePos>
									</Style>
									<ID>Turbo_ElevTrim</ID>
									<Title>ELEV</Title>
									<Unit></Unit>
									<Minimum>-100</Minimum>
									<Maximum>100</Maximum>
									<Value>
										<Substract>
											<Constant>0</Constant>
											<Simvar name="ELEVATOR TRIM PCT" unit="percent"/>
										</Substract>
									</Value>
									<ColorZone>
										<Color>green</Color>
										<Begin>
											<Add>
												<Substract>
													<Constant>0</Constant>
													<Gamevar name="AIRCRAFT ELEVATOR TRIM NEUTRAL" unit="percent"/>
												</Substract>
											<Constant>-15</Constant>
											</Add>
										</Begin>
										<End>									
											<Add>
												<Substract>
													<Constant>0</Constant>
													<Gamevar name="AIRCRAFT ELEVATOR TRIM NEUTRAL" unit="percent"/>
												</Substract>
												<Constant>15</Constant>
											</Add>
										</End>
									</ColorZone>
									<BeginText>UP</BeginText>
									<EndText>DN</EndText>
								</Gauge>
							</Column>
							<Column id="Horiz Trim">
								<Gauge>
									<Type>Horizontal</Type>
									<Style>
										<CursorColor>aqua</CursorColor>
										<Width>50</Width>
									</Style>
									<ID>Turbo_RudderTrim</ID>
									<Title>RUD</Title>
									<Unit></Unit>
									<Minimum>-100</Minimum>
									<Maximum>100</Maximum>
									<Value>
										<Simvar name="RUDDER TRIM PCT" unit="percent"/>
									</Value>
									<BeginText></BeginText>
									<EndText></EndText>
								</Gauge>
								<Gauge>
									<Type>Horizontal</Type>							
									<Style>
										<CursorColor>aqua</CursorColor>
										<Width>50</Width>
										<ReverseY>True</ReverseY>
									</Style>
									<ID>Turbo_AilTrim</ID>
									<Title>AIL</Title>
									<Unit></Unit>
									<Minimum>-100</Minimum>
									<Maximum>100</Maximum>
									<Value>
										<Simvar name="AILERON TRIM PCT" unit="percent"/>
									</Value>
									<ColorZone>
										<Color>white</Color>
										<Begin>-10</Begin>
										<End>10</End>
									</ColorZone>
									<BeginText></BeginText>
									<EndText></EndText>
								</Gauge>
							</Column>
						</ColumnsGroup>
					</Column>
					<Column width="33.3">
						<Text>
							<Center fontsize="20">
								<Content>Gear</Content>
								<Color>white</Color>
							</Center>
						</Text>
						<Text>
							<Center fontsize="24">
								<If>
									<Condition>
										<Lower>
											<Simvar name="GEAR CENTER POSITION" unit="percent"/>
											<Constant>100.0</Constant>
										</Lower>
									</Condition>
									<Then>
										<If>
											<Condition>
												<Greater>
													<Simvar name="GEAR CENTER POSITION" unit="percent"/>
													<Constant>0.0</Constant>
												</Greater>
											</Condition>
											<Then>
												<Constant>XIT</Constant>
											</Then>
											<Else>
												<Constant>UP</Constant>
											</Else>
										</If>
									</Then>
									<Else>
										<Constant>DOWN</Constant>
									</Else>
								</If>
							</Center>
						</Text>
					</Column>
				</ColumnsGroup>
				<ColumnsGroup id="Elec">
					<Column id="Bat" width="60">
						<ColumnsGroup>
